#include "main.h"

/**
 ** print_to_98 - Entry point
 ** @n: parameter
 ** Return: Always 0 (Success)
 **/

void print_to_98(int n)
{
	if (n > 99)
	{
	while (n > 99)
	{
		_putchar(n + '0');
		_putchar(',');
		_putchar(' ');
		n--;
	}
	}
	else if (n < 99)
	{
	while (n < 99)
	{
		_putchar(n + '0');
		_putchar(',');
		_putchar(' ');
		n++;
	}
	}
	else if (n == 98)
	{
		_putchar(n + '0');
	}
	_putchar('\n');
}
